rom \"@mui/material/Input\";\nimport InputAdornment from \"@mui/material/InputAdornment\";\n\nconst Header = () => {\n  const logout = (event, error) => {\n    event.preventDefault();\n    Auth.logout();\n\n    if (error) {\n      console.log(error);\n    }\n  };\n\n  return (\n    <header className=\"bg-primary text-light mb-4 flex \">\n      <div className=\"flex-row justify-space-between-lg justify-center align-center\">\n        <div>\n          <Link className=\"text-light\" to=\"/\">\n            <h1 className=\"m-0\">VodU</h1>\n            <p className=\"m-0\">Get into the mind of a gamer...</p>\n          </Link>\n        </div>\n\n        <FormControl variant=\"standard\">\n          <Input\n            placeholder=\"Search VodU\"\n            className=\"text-light\"\n            endAdornment={\n              <InputAdornment position=\"end\" className=\"text-light\">\n                <SearchIcon />\n              </InputAdornment>\n            }\n          />\n        </FormControl>\n\n        <div>\n          {Auth.loggedIn() ? (\n            <>\n              <Link className=\"text-light btn\" to=\"/me\">\n                {Auth.getProfile().data.username}'s profile\n              </Link>\n              <Link className=\"text-light btn\" variant=\"text\" onClick={logout}>\n                Logout\n              </Link>\n            </>\n          ) : (\n            <>\n              <Link variant=\"text\" className=\"text-light btn\" to=\"/login\">\n                Login\n              </Link>\n              <Link variant=\"text\" className=\"text-light btn\" to=\"/signup\">\n                Signup\n              </Link>\n            </>\n          )}\n        </div>\n      </div>\n    </header>\n  );\n};\n\nexport default Header;\n","C:\\Users\\Chris\\Documents\\finalProject2021\\repo\\client\\src\\utils\\mutations.js",[],"C:\\Users\\Chris\\Documents\\finalProject2021\\repo\\client\\src\\utils\\queries.js",[],"C:\\Users\\Chris\\Documents\\finalProject2021\\repo\\client\\src\\utils\\auth.js",[],"C:\\Users\\Chris\\Documents\\finalProject2021\\repo\\client\\src\\components\\ThoughtList\\index.js",[],"C:\\Users\\Chris\\Documents\\finalProject2021\\repo\\client\\src\\components\\ThoughtForm\\index.js",["86"],"import React, { useState } from 'react';\nimport { Link } from 'react-router-dom';\nimport { useMutation } from '@apollo/client';\n\nimport { ADD_THOUGHT } from '../../utils/mutations';\nimport { QUERY_THOUGHTS, QUERY_ME } from '../../utils/queries';\n\nimport Auth from '../../utils/auth';\n\nconst ThoughtForm = () => {\n  const [thoughtText, setThoughtText] = useState('');\n\n  const [characterCount, setCharacterCount] = useState(0);\n\n  const [addThought, { error }] = useMutation(ADD_THOUGHT, {\n    update(cache, { data: { addThought } }) {\n      try {\n        const { thoughts } = cache.readQuery({ query: QUERY_THOUGHTS });\n\n        cache.writeQuery({\n          query: QUERY_THOUGHTS,\n          data: { thoughts: [addThought, ...thoughts] },\n        });\n      } catch (e) {\n        console.error(e);\n      }\n\n      // update me object's cache\n      const { me } = cache.readQuery({ query: QUERY_ME });\n      cache.writeQuery({\n        query: QUERY_ME,\n        data: { me: { ...me, thoughts: [...me.thoughts, addThought] } },\n      });\n    },\n  });\n\n  const handleFormSubmit = async (event) => {\n    event.preventDefault();\n\n    try {\n      const { data } = await addThought({\n        variables: {\n          thoughtText,\n          thoughtAuthor: Auth.getProfile().data.username,\n        },\n      });\n\n      setThoughtText('');\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  const handleChange = (event) => {\n    const { name, value } = event.target;\n\n    if (name === 'thoughtText' && value.length <= 280) {\n      setThoughtText(value);\n      setCharacterCount(value.length);\n    }\n  };\n\n  return (\n    <div>\n      <h3>What's on your techy mind?</h3>\n\n      {Auth.loggedI